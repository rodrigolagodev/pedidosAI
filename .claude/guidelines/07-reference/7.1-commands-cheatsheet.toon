metadata:
  id: ref-commands
  version: 1.1
  lastUpdated: 2025-11-18
  references[0]:

content:
  title: Commands Cheatsheet
  purpose: Quick reference for commonly used development commands.

  sections[4]:
    - id: ref-cmd-dev
      title: Development
      commands[5]:
        - command: pnpm dev
          description: Start development server
        - command: pnpm build
          description: Build for production
        - command: pnpm start
          description: Start production server
        - command: pnpm lint
          description: Run ESLint
        - command: pnpm format
          description: Run Prettier

    - id: ref-cmd-test
      title: Testing
      commands[5]:
        - command: pnpm test
          description: Run unit tests
        - command: pnpm test --watch
          description: Run tests in watch mode
        - command: pnpm test:coverage
          description: Run tests with coverage
        - command: pnpm test:e2e
          description: Run E2E tests (Playwright)
        - command: pnpm test:e2e:ui
          description: E2E tests with UI

    - id: ref-cmd-supabase
      title: Supabase
      commands[4]:
        - command: pnpm dlx supabase gen types typescript --project-id wdtjhxxqgwobalxizlic > src/types/database.ts
          description: Generate TypeScript types
        - command: pnpm dlx supabase migration new [name]
          description: Create new migration
        - command: pnpm dlx supabase db push
          description: Apply migrations
        - command: pnpm dlx supabase functions deploy
          description: Deploy Edge Functions

    - id: ref-cmd-git
      title: Git
      commands[5]:
        - command: git status
          description: Check working tree status
        - command: git diff
          description: Show unstaged changes
        - command: git add -p
          description: Stage changes interactively
        - command: git commit -m "type: message"
          description: Commit with message
        - command: git push -u origin branch
          description: Push and set upstream
